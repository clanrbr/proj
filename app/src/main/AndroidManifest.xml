<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="localestates.localestates" >

    <uses-permission android:name="android.permission.INTERNET" />
    <permission
        android:name="localestates.localestates.permission.MAPS_RECEIVE"
        android:protectionLevel="signature" />

    <uses-permission android:name="android.permission.CALL_PHONE"></uses-permission>
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:largeHeap="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:parentActivityName=".StartActivity">
        <activity
            android:name=".StartActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AdvanceSearchActivity"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name=".SearchActivity"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name=".SearchResultActivity"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name=".AdvertActivity"
            android:label="" >
        </activity>
        <activity
            android:name=".MapActivity"
            android:label="" >
        </activity>

        <activity
            android:name=".AdvertImagesActivity"
            android:label="" >
        </activity>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <meta-data android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyCNBVx3m2Q0q2oo4FQhA4UdAyuTaCQ0BRg"/>
    </application>

</manifest>